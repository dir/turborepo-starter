{
  "name": "turborepo-starter",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo watch dev --continue",
    "lint": "turbo run lint --continue -- --cache --cache-location .cache/.eslintcache",
    "lint:fix": "turbo run lint --continue -- --fix --cache --cache-location .cache/.eslintcache",
    "style": "turbo run style --continue -- --check --cache --cache-location .cache/.prettiercache",
    "style:fix": "turbo run style --continue -- --write --cache --cache-location .cache/.prettiercache",
    "typecheck": "turbo run typecheck --parallel --continue",
    "ui:add": "turbo run ui:add",
    "ws:clean": "turbo run clean --parallel",
    "ws:outdated": "pnpm outdated --recursive",
    "ws:lint": "pnpm dlx sherif@latest",
    "clean": "git clean -xdf .cache .turbo node_modules",
    "postinstall": "pnpm ws:lint"
  },
  "devDependencies": {
    "@repo/prettier-config": "workspace:*",
    "turbo": "catalog:"
  },
  "engines": {
    "node": ">=20.18.1",
    "pnpm": "^9.15.2"
  },
  "packageManager": "pnpm@9.15.2",
  "prettier": "@repo/prettier-config"
}

{
  "$schema": "https://turborepo.org/schema.json",
  "ui": "tui",
  "tasks": {
    "clean": {
      "cache": false
    },
    "//#clean": {
      "cache": false
    },
    "check:types": {
      "outputs": [".cache/tsbuildinfo.json"]
    },
    "check:format": {
      "outputs": [".cache/.prettiercache"],
      "outputLogs": "new-only"
    },
    "check:all": {
      "dependsOn": ["^check:types", "^check:format"]
    }
  }
}
